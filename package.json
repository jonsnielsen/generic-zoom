  "devDependencies": {
    "@types/jest": "^24.0.21",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslint": "^6.0.0-beta0",
    "tslint-config-prettier": "^1.18.0",
